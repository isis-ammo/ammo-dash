<?xml version="1.0" encoding="utf-8"?>
<template type="uxo" label="UXO Report">
	<text id="Date &amp; Time Disc." prepop="time_formatted" />
	<location id="Location" hint="Tap Button to Grab Location" prepop="location" text="31NAA6602100000" />
	<text id="Radio Frequency" />
	<text id="Callsign" />
	<pulldown id="Type of Munitions">
		<choice>Dropped</choice>
		<choice>Projected</choice>
		<choice>Placed</choice>
		<choice>Thrown</choice>
	</pulldown>
	
	<pulldown id="NBC Contam. (Y/N)">
		<choice>Yes</choice>
		<choice>No</choice>
	</pulldown>

	<text id="Resources Threatened" />
	<text id="Impact on Mission" />
	<text id="How Identified" />

	<pulldown id="Recommended Priority" >
		<choice>Immediate</choice>
		<choice>Indirect</choice>
		<choice>Minor</choice>
		<choice>No Threat</choice>
	</pulldown>
</template>