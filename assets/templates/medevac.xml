<?xml version="1.0" encoding="utf-8"?>
<template type="medevac" label="MEDEVAC Report">
	<location id="Pick-up location" hint="Tap Button to Grab Location" prepop="location" text="31NAA6602100000" />
	<text id="Radio freq" />
	<text id="Callsign"/>
	<pulldown id="Precedence">
		<choice>Urgent</choice>
		<choice>Urgent Surgical</choice>
		<choice>Priority</choice>
		<choice>Routine</choice>
		<choice>Convenience</choice>
	</pulldown>
	
	<pulldown id="Sp.EQPMT REQ">
		<choice>None</choice>
		<choice>Hoist</choice>
		<choice>Extraction EQMT</choice>
		<choice>Ventilator</choice>
	</pulldown>

	<text id="Num. Pnts" />
	<pulldown id="Transpt Type">
		<choice>Litter</choice>
		<choice>Ambulatory</choice>
	</pulldown>
	
	<pulldown id="Pickup Site Marking">
		<choice>Panels</choice>
		<choice>Pyrotechnic Signal</choice>
		<choice>Smoke Signal</choice>
		<choice>None</choice>
		<choice>Other</choice>
	</pulldown>
	
	<pulldown id="Pnt Nationality &amp; Status">
		<choice>US Military</choice>
		<choice>US Civilian</choice>
		<choice>Non-US military</choice>
		<choice>Non-US civilian</choice>
		<choice>EPW</choice>
	</pulldown>
	
	<pulldown id="NBC Contamination">
		<choice>None</choice>
		<choice>Nuclear</choice>
		<choice>Biological</choice>
		<choice>Chemical</choice>
	</pulldown>
</template>